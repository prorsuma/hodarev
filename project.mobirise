max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n  </mbr-parameters>\n\n  <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n  <div class=\"container\">\n    <div class=\"mbr-row\">\n      <div class=\"mbr-col-lg-6 mbr-col-md-12 mbr-col-sm-12 md-pb\">\n        <h3 class=\"mbr-text mbr-bold text-primary mbr-fonts-style mbr-pb-2 group-title\" mbr-theme-style=\"display-5\" data-app-selector=\".group-title\" mbr-if=\"showTitle\">Hodarev Holding</h3>\n        <p class=\"mbr-text mbr-white mbr-light py-2 links mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".links\">We invest in IT</p>\n        <div class=\"icons-list mbr-pt-2 mbr-flex flex-wrap\" mbr-list mbr-list-grow=\"iconsCount\" mbr-link-type=\"socials\" mbr-if=\"showIcons\">\n          <a href=\"https://mobirise.com\" target=\"_blank\">\n            <span class=\"iconfont-wrapper\">\n              <span mbr-icon class=\"amp-iconfont animation-normal fa-envelope fa\"></span></span>\n          </a>\n          \n          \n          \n          \n          \n        </div>\n      </div>\n      <div class=\"mbr-col-lg-3 mbr-col-md-6 mbr-col-sm-12 md-pb\" mbr-if=\"list1Items1!=0\">\n        <h3 class=\"mbr-text mbr-bolder text-primary mbr-fonts-style mbr-pb-2 mbr-black group-title\" mbr-theme-style=\"display-7\" data-app-selector=\".group-title\" mbr-if=\"showTitle\"></h3>\n        <div class=\"mbr-row items \" mbr-list mbr-list-grow=\"list1Items1\">\n\n          <div class=\"link-ws-black mbr-light mbr-white list-item mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12 mbr-pt-1\" mbr-theme-style=\"display-7\" data-app-selector=\".link-ws-black a\">\n            <a href=\"#\" mbr-text class=\"text-white\">Project Expertize</a>\n          </div>\n\n        </div>\n      </div>\n      <div class=\"mbr-col-lg-3 mbr-col-md-6 mbr-col-sm-12\" mbr-if=\"list1Items2!=0\">\n        <h3 class=\"mbr-text mbr-bolder mbr-fonts-style mbr-pb-2 text-primary group-title\" mbr-theme-style=\"display-7\" data-app-selector=\".group-title\" mbr-if=\"showTitle\">INFO</h3>\n        <div class=\"mbr-row items \" mbr-list mbr-list-grow=\"list1Items2\">\n          <div class=\"link-ws-black mbr-light mbr-white list-item mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12 mbr-pt-1\" mbr-theme-style=\"display-7\" data-app-selector=\".link-ws-black a\">\n            <a href=\"#\" mbr-text class=\"text-white\">Project expertize</a>\n          </div>\n          <div class=\"link-ws-black mbr-light mbr-white list-item mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12 mbr-pt-1\" mbr-theme-style=\"display-7\" data-app-selector=\".link-ws-black a\">\n            <a href=\"#\" mbr-text class=\"text-white\">Stock Trading</a>\n          </div>\n          \n        <div class=\"link-ws-black mbr-light mbr-white list-item mbr-col-lg-12 mbr-col-md-12 mbr-col-sm-12 mbr-pt-1\" mbr-theme-style=\"display-7\" data-app-selector=\".link-ws-black a\">\n            <a href=\"#\" mbr-text class=\"text-white\">Consalting</a>\n          </div></div>\n      </div>\n    </div>\n    <div class=\"copyright mbr-pt-5 mbr-flex mbr-jc-c\" mbr-if=\"showCopyright\">\n        <p class=\"mbr-text mbr-fonts-style mbr-light mbr-white align-center\" mbr-theme-style=\"display-7\" data-app-selector=\".copyright .mbr-text\" data-app-placeholder=\"Type Text\"></p>\n    </div>      \n  </div>\n</section>",
